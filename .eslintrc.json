{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "arrow-body-style": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0,
        "no-console": 0,
        "no-param-reassign": 0,
        "react/jsx-one-expression-per-line": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}
